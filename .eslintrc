{
  parser: "babel-eslint",
  "extends": "airbnb",
  "rules": {
    "react/require-extension": "off",
    "linebreak-style": "off",
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "no-unused-vars": "off",
    "no-alert": "off",
  },
  "env": {
    "browser": true
  }
}
